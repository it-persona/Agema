<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta name="description" content="">
    <meta name="author" content="">
    <title>{% block title %} Admin Dashboard Â· User Registration {% endblock %}</title>
    {% block stylesheets %}
        <!-- Bootstrap Core CSS -->
        <link href="{{ asset('bundles/panchagema/css/bootstrap.min.css') }}" rel="stylesheet">
        <!-- MetisMenu CSS -->
        <link href="{{ asset('bundles/panchagemaadmin/bower_components/metisMenu/dist/metisMenu.min.css') }}" rel="stylesheet">
        <!-- Timeline CSS -->
        <link href="{{ asset('bundles/panchagemaadmin/dist/css/timeline.css') }}" rel="stylesheet">
        <!-- Custom CSS -->
        <link href="{{ asset('bundles/panchagemaadmin/dist/css/sb-admin-2.css') }}" rel="stylesheet">
        <!-- Morris Charts CSS -->
        <link href="{{ asset('bundles/panchagemaadmin/bower_components/morrisjs/morris.css') }}" rel="stylesheet">
        <!-- Custom Fonts -->
        <link href="{{ asset('bundles/panchagemaadmin/bower_components/font-awesome/css/font-awesome.min.css') }}" rel="stylesheet" type="text/css">
        <!-- Bootstrap mod by IT-PERSONA-->
        <link href="{{ asset('bundles/panchagemaadmin/styles-mod.css') }}" rel="stylesheet" type="text/css">
        <!-- Social Buttons CSS -->
        <link href="{{ asset('bundles/panchagemaadmin/bower_components/bootstrap-social/bootstrap-social.css') }}" rel="stylesheet">
        <!-- HTML5 Shim and Respond.js IE8 support of HTML5 elements and media queries -->
        <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
        <!--[if lt IE 9]>
        <script src="https://oss.maxcdn.com/libs/html5shiv/3.7.0/html5shiv.js"></script>
        <script src="https://oss.maxcdn.com/libs/respond.js/1.4.2/respond.min.js"></script>
        <![endif]-->
    {% endblock stylesheets %}
    <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}" />
</head>
<body>

{% block body %}
<div id="wrapper">
    {% block navbar %}
        {% include '@PanchAgemaAdmin/includes/navbar.html.twig' %}
    {% endblock navbar %}
    <div id="page-wrapper">
        {% block fos_user_content %}{% endblock %}
        <footer>
            <small>&copy; 2015 IT-PERSONA, Lab.</small>
        </footer>
    </div>
    <!-- /#page-wrapper -->
<!-- /#page-wrapper -->
</div>
{% endblock body %}

{% block javascripts %}
    <!-- jQuery -->
    <script src="{{ asset('bundles/panchagema/js/jquery-1.11.1.min.js') }}"></script>
    <!-- Bootstrap Core JavaScript -->
    <script src="{{ asset('bundles/panchagema/js/bootstrap.min.js') }}"></script>
    <!-- Metis Menu Plugin JavaScript -->
    <script src="{{ asset('bundles/panchagemaadmin/bower_components/metisMenu/dist/metisMenu.min.js') }}"></script>
    <!-- Custom Theme JavaScript -->
    <script src="{{ asset('bundles/panchagemaadmin/dist/js/sb-admin-2.js') }}"></script>
{% endblock javascripts %}

